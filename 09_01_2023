#include<iostream>//Librería de cabecera
//Instrucciones básicas
//for

using namespace std;

int main(){
	int cont;
	for(cont=1;cont<=4;cont++){
		cout<<"Estoy contento"<<cont;
	}
	return 0;
	
	
}


#include <iostream>
using namespace std;

int main(){
    int cont;
    int n_tabla;
    cout<<"Dime que tabla deseas: ";
    cin>>n_tabla;
    cout<<"**********************"<<endl;
    cout<<"*     Tabla del  "<<n_tabla<<"   *"<<endl;
    cout<<"**********************"<<endl;
    for(cont=0;cont<=10;cont++){
        cout<<n_tabla<<" x "<<cont<<" = "<<n_tabla*cont<<endl;
    }
    
    return 0;
}
